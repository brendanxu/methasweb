{
  "version": 2,
  "name": "methas-southpole",
  "builds": [
    {
      "src": "apps/main-site/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/main-site/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "functions": {
    "apps/main-site/app/**/*.ts": {
      "maxDuration": 30
    }
  }
}